
<div class="container">
  <h1>All Posts</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= link_to post.title,post_path(post) %></td>
        <td><%= post.body.slice(0..25)+".." %></td>
      </tr>
  <% end %>
  </tbody>
</table>
</div>
<%= will_paginate @posts %>
