<div class="container">
  <h1>Sign up</h1>
    <%= @user.errors.full_messages.join(" ,") %>
  <%= simple_form_for @user do |f| %>
    <%= f.input  :first_name%>
    <%= f.input  :last_name%>
    <%= f.input  :email%>
    <%= f.input :password%>
    <%= f.input :password_confirmation%>
  <%= f.button :submit %>
  <% end %>
</div>
